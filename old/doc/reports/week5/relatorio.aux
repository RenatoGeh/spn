\@writefile{toc}{\defcounter {refsection}{0}\relax }\@writefile{toc}{\contentsline {paragraph}{ Agora que temos uma no\IeC {\c c}\IeC {\~a}o b\IeC {\'a}sica sobre grafos podemos definir o grafo de intera\IeC {\c c}\IeC {\~a}o. }{3}{paragraph*.15}}
\@writefile{toc}{\defcounter {refsection}{0}\relax }\@writefile{toc}{\contentsline {paragraph}{ Em outras palavras, as vari\IeC {\'a}veis $\textbf  {X}_i$ de cada factor $f_i$ formam um clique em $G$, ou seja, as vari\IeC {\'a}veis emparelhadas s\IeC {\~a}o adjacentes. }{3}{paragraph*.16}}
